	}
	
	public static boolean testArray(int a, int b, int c)
	{
		this.num0 = a;
		
		if ( num0 == a)
		{
			return true;
		}
		else if ( num1 == b)
		{
			return true;
		}
		else if ( num2 == c)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
}