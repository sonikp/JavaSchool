public class TestCode
{
	public static void main(String[] args)
	{
		// command line arguments
		int num0 = 5;
		int num1 = 7;
		int num2 = 9;
		
		// create IntegerSet Array
		int[] IntegerSet = new int[3];
		// create Boolean set array called SetA
		boolean[] SetA = new boolean[3];
		
		// Display initial values
		System.out.println("-----Before Initialized-----");
		System.out.print(IntegerSet[0] + " ");
		System.out.print(IntegerSet[1] + " ");
		System.out.print(IntegerSet[2] + " ");
		System.out.println();
		System.out.print(SetA[0] + " ");
		System.out.print(SetA[1] + " ");
		System.out.print(SetA[2] + " ");
		System.out.println();
		
		// assign command line arguments to IntegerSet Array
		IntegerSet[0] = num0;
		IntegerSet[1] = num1;
		IntegerSet[2] = num2;
		
		// Assign command line arguments to IntegerSet Array
		System.out.println("-----After Initialized-----");
		System.out.print(IntegerSet[0] + " ");
		System.out.print(IntegerSet[1] + " ");
		System.out.print(IntegerSet[2] + " ");
		
		
		boolean result = testArray(5,7,9);
		
		System.out.println("\nresult: \t" + result);
		
